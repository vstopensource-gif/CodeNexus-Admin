rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    function isAdmin() {
      return request.auth != null && request.auth.token.email == 'vstopensource@gmail.com';
    }

    // Lock down everything to the admin only
    match /{document=**} {
      allow read, write: if isAdmin();
    }
  }
}

